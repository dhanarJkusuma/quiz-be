<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>QuizApp | Dashboard</title>

    {{ template "_admin_source" }}
</head>
<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">

        {{ template "_admin_navbar" }}

        {{ template "_admin_sidebar" . }}

        <div class="content-wrapper">

            {{ template "_admin_header" }}

            <section class="content">
                <div class="container-fluid">

                    {{ template "_admin_dashboard_card" }}

                    <div class="row">
                        <section class="col-lg-12 connectedSortable">
                            <div class="card">
                                <div class="card-header">
                                    <h3 class="card-title">
                                        <i class="nav-icon fas fa-tachometer-alt"></i>
                                        Dashboard
                                    </h3>
                                    <div class="card-tools">

                                    </div>
                                </div>
                                <div class="card-body">
                                    <div class="tab-content p-0">
                                        Welcome to Quiz App Dashboard Admin
                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>

                </div>
            </section>
        </div>

        {{ template "_admin_footer" }}

        <aside class="control-sidebar control-sidebar-dark"></aside>
    </div>


    {{ template "_admin_script" . }}
</body>
</html>
